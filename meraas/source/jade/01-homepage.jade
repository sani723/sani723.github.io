                                        a.link(href="#"): span watch video
                                        a.link(href="#"): span chairman message

                                    a.more
                                        span learn more about
                                        span.icon
            // End .wrapper-mod for featured projects

            // Start .wrapper-mod for our brands carousel
            .wrapper-mod.full-width
                .mod-content
                    .mod-brands-carousel
                        h2.middle-items: span our brands

                        .brands-list
